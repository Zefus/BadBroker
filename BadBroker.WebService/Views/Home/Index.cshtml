@{
    ViewData["Title"] = "Home Page";
}

<div class="container" style="margin-top:10px">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="form-group">
                <div class="input-group date" id="startDate">
                    <input type="text" class="form-control" />
                    <span class="input-group-addon">
                        <span class="glyphicon-calendar glyphicon"></span>
                    </span>
                </div>
            </div>
            <div class="form-group">
                <div class="input-group date" id="endDate">
                    <input type="text" class="form-control" />
                    <span class="input-group-addon">
                        <span class="glyphicon-calendar glyphicon"></span>
                    </span>
                </div>
            </div>
            <div>
                <button class="btn btn-primary" id="trade">Trade</button>
            </div>
            <table class="table table-bordered" style="visibility:hidden" id="results">
                <tbody>
                    <tr>
                        <td>Currency</td>
                        <td><div id="currency"></div></td>
                    </tr>
                    <tr>
                        <td>Buy Date</td>
                        <td><div id="buyDate"></div></td>
                    </tr>
                    <tr>
                        <td>Sell Date</td>
                        <td><div id="sellDate"></div></td>
                    </tr>
                    <tr>
                        <td>Revenue USD</td>
                        <td><div id="revenueUSD"></div></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
